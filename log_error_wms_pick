	[IdPickingEnc] [int] NULL,
	[IdPickingDet] [int] NULL,
	[IdPickingUbic] [int] NULL,
	[IdPedidoEnc] [int] NULL,
	[IdPedidoDet] [int] NULL,
	[CodigoProducto] [nvarchar](100) NULL,
	[NombreProducto] [nvarchar](500) NULL,
	[Cantidad_Recibida] [float] NULL,
	[User_Agr] [int] NULL,
