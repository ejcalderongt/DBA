/********************** GT24032022  campos de poliza en pedido, datos del vehiculo en despacho ************************/


Alter view VW_Movimientos_N as
SELECT pr.nombre_comercial AS Propietario, p.nombre AS Producto, enc.codigo_poliza AS Poliza, pp.nombre AS Presentación, pe1.nombre AS EstadoOrigen, pe2.nombre AS EstadoDestino, u.Nombre AS UMBas, m.cantidad, m.peso, m.lote, 
dbo.Nombre_Completo_Ubicacion(u1.IdUbicacion, u1.IdBodega) AS UbicOrigen, dbo.Nombre_Completo_Ubicacion(u2.IdUbicacion, u2.IdBodega) AS UbicDestino, stt.Nombre AS TipoTarea, m.fecha, p.IdProducto, p.codigo, 
p.codigo_barra AS CodigoBarra, stt.IdTipoTarea, stt.Contabilizar, m.fecha_vence, pr.IdTipoActualizacionCosto, m.IdPresentacion, m.IdUnidadMedida, m.IdEstadoOrigen, m.IdProductoBodega, prb.IdPropietarioBodega, prb.IdBodega, 
m.barra_pallet, dbo.producto_clasificacion.nombre AS Clasificacion, dbo.producto_familia.nombre AS Familia, m.IdBodegaOrigen, m.IdBodegaDestino, bodega_1.codigo AS Codigo_Bodega_Destino, 
bodega_1.nombre AS Nombre_Bodega_Destino, m.IdMovimiento, dbo.bodega.codigo AS Codigo_Bodega_Origen, dbo.bodega.nombre AS Nombre_Bodega_Origen, dbo.Nombre_Area(u1.IdArea, u1.IdBodega) AS Area_Origen, 
enc.IdRegimen AS IdRegimenIngreso, dbo.regimen_fiscal.codigo_regimen AS regimenIngreso, dbo.trans_oc_enc.no_ticket_tms, dbo.tms_ticket.Fecha_Ingreso, dbo.empresa_transporte_vehiculos.placa, 
dbo.empresa_transporte_vehiculos.marca, dbo.empresa_transporte_vehiculos.tipo, 
dbo.tipo_contenedor.nombre as contendor_ingreso,
dbo.trans_despacho_enc.IdVehiculo, dbo.trans_despacho_enc.IdPiloto, dbo.trans_pe_pol.IdOrdenPedidoEnc, 
dbo.trans_pe_pol.NoPoliza AS Poliza_Salida,transporte_vehiculos2.placa AS placa_salida,transporte_vehiculos2.marca AS marca_salida,transporte_vehiculos2.tipo as tipo_salida,
tipo_contenedor2.Nombre as contenedor_salida,
dbo.trans_despacho_enc.fec_agr AS Fecha_Salida, dbo.trans_despacho_det.CantidadDespachada,
regimen_fiscal2.codigo_regimen as Regimen_Salida
FROM     dbo.producto_presentacion AS pp RIGHT OUTER JOIN
dbo.bodega_ubicacion AS u1 RIGHT OUTER JOIN
dbo.trans_despacho_det INNER JOIN
dbo.trans_despacho_enc ON dbo.trans_despacho_det.IdDespachoEnc = dbo.trans_despacho_enc.IdDespachoEnc INNER JOIN
dbo.trans_pe_pol ON dbo.trans_despacho_det.IdPedidoEnc = dbo.trans_pe_pol.IdOrdenPedidoEnc RIGHT OUTER JOIN
dbo.trans_movimientos AS m LEFT OUTER JOIN
dbo.propietario_bodega AS prb ON m.IdPropietarioBodega = prb.IdPropietarioBodega INNER JOIN
dbo.propietarios AS pr ON prb.IdPropietario = pr.IdPropietario LEFT OUTER JOIN
dbo.producto_bodega AS pb ON m.IdProductoBodega = pb.IdProductoBodega INNER JOIN
dbo.producto AS p ON pb.IdProducto = p.IdProducto INNER JOIN
dbo.trans_oc_enc ON prb.IdPropietarioBodega = dbo.trans_oc_enc.IdPropietarioBodega INNER JOIN
dbo.tms_ticket ON pr.IdPropietario = dbo.tms_ticket.IdPropietario AND dbo.trans_oc_enc.no_ticket_tms = dbo.tms_ticket.IdTicket INNER JOIN
dbo.empresa_transporte_vehiculos ON dbo.tms_ticket.IdVehiculo = dbo.empresa_transporte_vehiculos.IdVehiculo ON dbo.trans_despacho_det.IdDespachoEnc = m.IdTransaccion LEFT OUTER JOIN
dbo.bodega AS bodega_1 ON m.IdEmpresa = bodega_1.IdEmpresa AND m.IdBodegaDestino = bodega_1.IdBodega LEFT OUTER JOIN
dbo.bodega ON m.IdBodegaOrigen = dbo.bodega.IdBodega AND m.IdEmpresa = dbo.bodega.IdEmpresa LEFT OUTER JOIN
dbo.producto_clasificacion ON p.IdClasificacion = dbo.producto_clasificacion.IdClasificacion LEFT OUTER JOIN
dbo.producto_familia ON p.IdFamilia = dbo.producto_familia.IdFamilia LEFT OUTER JOIN
dbo.bodega_ubicacion AS u2 ON m.IdUbicacionDestino = u2.IdUbicacion AND m.IdBodegaDestino = u2.IdBodega ON u1.IdUbicacion = m.IdUbicacionOrigen AND u1.IdBodega = m.IdBodegaOrigen ON 
pp.IdPresentacion = m.IdPresentacion LEFT OUTER JOIN
dbo.producto_estado AS pe1 ON m.IdEstadoOrigen = pe1.IdEstado LEFT OUTER JOIN
dbo.producto_estado AS pe2 ON m.IdEstadoDestino = pe2.IdEstado LEFT OUTER JOIN
dbo.unidad_medida AS u ON m.IdUnidadMedida = u.IdUnidadMedida LEFT OUTER JOIN
dbo.sis_tipo_tarea AS stt ON m.IdTipoTarea = stt.IdTipoTarea FULL OUTER JOIN
dbo.trans_re_oc AS re ON dbo.trans_oc_enc.IdOrdenCompraEnc = re.IdOrdenCompraEnc AND m.IdRecepcion = re.IdRecepcionEnc FULL OUTER JOIN
dbo.regimen_fiscal INNER JOIN
dbo.trans_oc_pol AS enc ON dbo.regimen_fiscal.IdRegimen = enc.IdRegimen ON re.IdOrdenCompraEnc = enc.IdOrdenCompraEnc LEFT OUTER JOIN
dbo.regimen_fiscal as regimen_fiscal2 on dbo.trans_pe_pol.IdRegimen = regimen_fiscal2.IdRegimen LEFT OUTER JOIN
dbo.empresa_transporte_vehiculos transporte_vehiculos2 on dbo.trans_despacho_enc.IdVehiculo= transporte_vehiculos2.IdVehiculo INNER JOIN
dbo.tipo_contenedor on dbo.empresa_transporte_vehiculos.IdTipoContenedor = dbo.tipo_contenedor.IdTipoContenedor INNER JOIN
dbo.tipo_contenedor as tipo_contenedor2 on dbo.empresa_transporte_vehiculos.IdTipoContenedor= tipo_contenedor2.IdTipoContenedor


