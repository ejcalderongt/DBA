                         prov.nombre AS Nombre_Proveedor, oc_enc.No_Documento AS No_DocumentoOC, oc_enc.Referencia AS ReferenciaOC, re_enc.fecha_recepcion AS Fecha, re_enc.estado, re_tr.TipoTrans, re_tr.Descripcion, re_enc.activo, 
