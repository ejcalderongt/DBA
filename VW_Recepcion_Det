                  um.Nombre AS UM, pr_estado.nombre AS EstadoProd, prod_pr.nombre AS PresProd, re_det.lic_plate AS Licencia, re_det.lote, re_det.fecha_vence, oc_enc.IdPedidoEncDevolucion, oc_enc.no_documento_devolucion, 
