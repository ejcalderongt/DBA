                         re_det.cantidad_recibida AS Recibido, um.Nombre AS UM, pr_estado.nombre AS EstadoProd, prod_pr.nombre AS PresProd, re_det.lic_plate, re_det.lote, re_det.fecha_vence, oc_enc.IdPedidoEncDevolucion, 
                         oc_enc.No_Documento_Devolucion, dbo.cliente.nombre_comercial AS Nombre_Cliente_Devolucion
FROM            dbo.trans_pe_enc RIGHT OUTER JOIN
