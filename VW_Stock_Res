                         oc_pol.numero_orden, oc_pol.codigo_poliza, re_oc.IdOrdenCompraEnc, dbo.bodega_ubicacion.ubicacion_picking, dbo.producto_presentacion.CamasPorTarima, dbo.producto_presentacion.CajasPorCama
