                  ISNULL(dbo.stock.cantidad / IIF(dbo.producto_presentacion.factor = 0, 1, dbo.producto_presentacion.factor), 0) AS Cantidad_Presentacion, dbo.producto_estado.nombre AS NomEstado, 
