CREATE TABLE color ( IdColor INT PRIMARY KEY, Nombre NVARCHAR(50) NOT NULL, CodigoHex NVARCHAR(7) NULL,  IdPropietario INT NOT NULL,  fec_agr DATETIME NOT NULL DEFAULT GETDATE(), user_agr NVARCHAR(25) NOT NULL, fec_mod DATETIME NOT NULL DEFAULT GETDATE(), user_mod NVARCHAR(25) NULL, activo bit default 0 );
