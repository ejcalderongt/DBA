CREATE TABLE [dbo].[trans_picking_prioridad](
	[IdPrioridadPicking] [int] NOT NULL,
	[Codigo] [int] NULL,
	[Nombre] [nvarchar](50) NULL,
	[user_agr] [nvarchar](30) NULL,
	[fec_agr] [datetime] NULL,
	[user_mod] [nvarchar](30) NULL,
	[fec_mod] [datetime] NULL,
	[activo] [bit] NULL,
