	ALTER TABLE trans_ubic_hh_stock DROP CONSTRAINT FK_stock_ubic_hh_bodega_ubicacion
	ALTER TABLE trans_ubic_hh_stock DROP CONSTRAINT FK_stock_ubic_hh_bodega_ubicacion_rec
	ALTER TABLE trans_ubic_hh_stock DROP CONSTRAINT FK_stock_ubic_hh_bodega_ubicacion1
	ALTER TABLE trans_ubic_hh_stock DROP CONSTRAINT FK_stock_ubic_hh_bodega_ubicacion1_rec
	ALTER TABLE transacciones_log DROP CONSTRAINT FK_transacciones_log_bodega_ubicacion
	ALTER TABLE BODEGA_UBICACION DROP CONSTRAINT PK_bodega_ubicacion_1

   ALTER TABLE bodega_area
   ADD CONSTRAINT PK_BodegaAreaId PRIMARY KEY CLUSTERED (IdArea, IdBodega);

   ALTER TABLE bodega_sector ADD IdBodega int NOT NULL DEFAULT 0
   
   update bodega_sector set idbodega = 1 
   where idarea in (select idarea from bodega_area where idbodega =1)

   update bodega_sector set idbodega = 3
   where idarea in (select idarea from bodega_area where idbodega =3)

   update bodega_sector set idbodega = 4
   where idarea in (select idarea from bodega_area where idbodega =4)
   
   update bodega_sector set idbodega = 5
   where idarea in (select idarea from bodega_area where idbodega =5)

   ALTER TABLE bodega_sector
   ADD CONSTRAINT PK_BodegaSectorId PRIMARY KEY CLUSTERED (IdSector, IdArea, IdBodega);

   ALTER TABLE bodega_tramo ADD IdArea int NOT NULL DEFAULT 0
   ALTER TABLE bodega_tramo ADD IdBodega int NOT NULL DEFAULT 0

	UPDATE bt
	SET bt.IdBodega = bs.IdBodega,
	bt.IdArea = bs.IdArea
	FROM bodega_tramo bt
	INNER JOIN bodega_sector bs	
	ON bt.IdSector =  bs.IdSector;

	ALTER TABLE bodega_tramo
    ADD CONSTRAINT PK_BodegaTramoId PRIMARY KEY CLUSTERED (IdTramo, IdSector, IdArea, IdBodega);
	
    ALTER TABLE bodega_ubicacion ADD IdBodega int NOT NULL DEFAULT 0
	ALTER TABLE bodega_ubicacion ADD IdArea int NOT NULL DEFAULT 0
	ALTER TABLE bodega_ubicacion ADD IdSector int NOT NULL DEFAULT 0	
	ALTER TABLE bodega_ubicacion ALTER COLUMN IdTramo int NOT NULL

	UPDATE bu
	SET bu.IdBodega = bt.IdBodega,
	bu.IdArea = bt.IdArea,
	bu.IdSector = bt.IdSector,
	bu.IdTramo = bt.IdTramo
	FROM bodega_ubicacion bu
	INNER JOIN bodega_tramo bt
	ON bt.IdTramo =  bt.IdTramo;

	ALTER TABLE bodega_ubicacion
    ADD CONSTRAINT PK_BodegaUbicacionId PRIMARY KEY CLUSTERED (IdUbicacion, IdTramo, IdSector, IdArea, IdBodega);
